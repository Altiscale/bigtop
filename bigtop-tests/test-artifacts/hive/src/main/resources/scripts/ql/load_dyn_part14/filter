sed -e '/^explain/,/Stage: Stage-4/d' \
                -e '/# Partition Information/,/^value/d' \
                -e '/Stage: Stage-6/,/Stage: Stage-10/d' \
                -e '/Map Reduce/,/output format: /d' \
                -e '/serde: /d' -e '/name: default.nzhang_part14/d'  \
                -e  's#owner:[^,]*,#owner:BORG,#' \
                -e  's#createTime:[0-9]*,#createTime:JUSTNOW#' \
                -e  's#location:hdfs://.*/nzhang_part14,#location:hdfs://HADOOP/nzhang_part14,#' | 
sed  -e  's#{transient_lastDdlTime=[0-9]*}#{transient_lastDdlTime=JUSTNOW}#' \
     -e '/^[ \t]\+$/d'

